{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@config/*": ["src/config/*"],
      "@helpers/*": ["src/helpers/*"],
      "@models/*": ["src/models/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"]
    },
    "types": ["jest", "@types/styled-components-react-native"]
  }
}
