{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components/index"],
      "@configs/*": ["src/configs/*"],
      "@contexts/*": ["src/contexts/*"],
      "@models/*": ["src/models/*"],
      "@src/*": ["src/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"]
    },
    "types": ["jest", "@types/styled-components-react-native"]
  }
}
